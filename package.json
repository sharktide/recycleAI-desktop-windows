{
  "name": "RecycleAI",
  "version": "1.1.1",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "dist": "electron-builder --win"
  },
  "build": {
    "appId": "com.sharktide.RecycleAI",
    "productName": "RecycleAI Windows",
    "files": [
      "**/*",
      "dist/**/*",
      "index.html",
      "main.js",
      "scripts.js",
      "styles.css"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "axios": "^1.7.9",
    "child_process": "^1.0.2",
    "electron-log": "^5.2.4",
    "fs": "^0.0.1-security",
    "https": "^1.0.0",
    "os": "^0.1.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8"
  }
}
